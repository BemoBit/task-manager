    "@nestjs/testing": "^10.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/handlebars": "^4.0.40",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
